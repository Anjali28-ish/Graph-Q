link:https://www.geeksforgeeks.org/problems/depth-first-traversal-for-a-graph/1

class Solution {
    static void dfsRec(ArrayList<ArrayList<Integer>> adj,
           boolean[] visited, int curr, ArrayList<Integer> res)
    {
        visited[curr] = true;
        res.add(curr);

        
        for (int i = 0;i<adj.get(curr).size();i++) {
            int neigh = adj.get(curr).get(i);
            if (!visited[neigh]) {
                dfsRec(adj, visited, neigh , res);
            }
        }
    }

    static ArrayList<Integer>dfs(ArrayList<ArrayList<Integer>> adj) {
        boolean[] visited = new boolean[adj.size()];
        ArrayList<Integer> res = new ArrayList<>();
        dfsRec(adj, visited, 0, res);
        return res;
    }
        
        
    
}